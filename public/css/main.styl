/*==============================
	Profile
==============================*/
.profile
	position relative
	display flex
	flex-direction column
	justify-content flex-start
	align-items flex-start
	padding 20px
	border-radius 6px
	box-shadow 0 1px 15px 0 rgba(0, 0, 0, .12)
	margin-bottom 10px
	&__tabs
		display flex
		flex-direction row
		justify-content flex-start
		align-items center
		overflow hidden
		border-bottom 1px solid rgba($grey, .7)
		margin-bottom 30px
		width 100%
		li
			margin-right 40px
