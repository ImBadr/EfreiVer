	&__input
		background-color rgba($grey, .12)
		border 1px solid rgba($grey, .7)
		border-radius 6px
		height 44px
		position relative
		color $text1
		font-size 14px
		width 100%
		padding 0 20px
		letter-spacing .4px
		&:focus
			border-color $purple
	&__checkboxes
		position relative
		margin-top 5px
		li
			position relative
			margin-bottom 15px
			&:last-child
				margin-bottom 0
		input
			&:not(:checked)
			&:checked
				position absolute
				left -9999px
			&:not(:checked) + label
			&:checked + label
				font-size 14px
				color $text1
				font-weight normal
				position relative
				cursor pointer
				padding-left 35px
				line-height 20px
				letter-spacing .4px
				margin 0
				a
					color $purple
					&:hover
						color $purple
			&:not(:checked) + label:before
			&:checked + label:before
				content ''
				position absolute
				left 0
				top 0
				width 20px
				height 20px
				border 1px solid rgba($grey, .7)
				border-radius 6px
				background-color rgba($grey, .12)
			&:not(:checked) + label:after
			&:checked + label:after
				content ''
				position absolute
				left 0
				top 0
				width 20px
				height 20px
				text-align center
				transition .5s
				background url('../img/checkmark.svg') no-repeat center / 18px auto
			&:not(:checked) + label:after
				opacity 0
				transform scale(0)
			&:checked + label:after
				opacity 1
				transform scale(1)
		label
			&::-moz-selection
				background transparent
				color $text1
			&::selection
				background transparent
				color $text1
