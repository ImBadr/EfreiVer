$white = #fff
$black = #000
$red = #fd6060
$green = #29b474
$green2 = #199c60
$purple = #795ab0
$bg = #fff
$orange = #f26c2a
$grey = #dbdada
$text1 = #33373f
$text2 = #585963

/*==============================
	Fonts
==============================*/
@import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500&family=Open+Sans:wght@400;600&display=swap')

/*==============================
	Common styles
==============================*/
html
	-webkit-text-size-adjust 100%
	-ms-text-size-adjust 100%
html
body
	height 100%
body
	font-family 'Open Sans', sans-serif
	font-weight 400
	-webkit-font-smoothing antialiased
	background-color $bg
button
	padding 0
	border none
	background-color transparent
	transition .5s
	cursor pointer
	&:focus
		outline none
a
	transition .5s
	&:hover
	&:active
	&:focus
		outline none
		text-decoration none
input
textarea
select
	padding 0
	margin 0
	border-radius 0
	-webkit-appearance none
	-moz-appearance none
	-ms-appearance none
	appearance none
	box-shadow none
	transition .5s
	&:focus
		outline none

select::-ms-expand
	display none

ul
	margin 0
	padding 0
	list-style none

::-moz-selection
	background $purple
	color $white
	text-shadow none
::selection
	background $purple
	color $white
	text-shadow none

::-webkit-input-placeholder
	color $text2
	opacity 1
::-moz-placeholder
	color $text2
	opacity 1
:-moz-placeholder
	color $text2
	opacity 1
:-ms-input-placeholder
	color $text2
	opacity 1

.container
	@media (min-width: 1310px)
		max-width 1310px

.owl-carousel .owl-item img
	width auto
	max-width 100%

.tab-content > .tab-pane
	display none

.tab-content > .active
	display block

.fade
	transition opacity .3s linear

@media screen and (prefers-reduced-motion: reduce)
	.fade
		transition none

.fade:not(.show)
	opacity 0

.table-responsive
	display block
	width 100%
	overflow-x auto
	-webkit-overflow-scrolling touch
	-ms-overflow-style -ms-autohiding-scrollbar
	&--border
		box-shadow 0 1px 15px 0 rgba(0, 0, 0, .12)
		border-radius 6px
		margin-top 30px

/*==============================
	Header
==============================*/
.header
	position fixed
	top 0
	left 0
	width 100%
	background-color $bg
	z-index 99
	transition .5s, margin 0s
	box-shadow 0 1px 15px 0 rgba(0, 0, 0, .12)
	&--menu
		top 250px
	&--scroll
		top -71px
	&__wrap
		border-bottom 1px solid rgba($grey, .7)
		&:last-child
			border-color transparent
	&__content
		display flex
		flex-direction row
		justify-content space-between
		align-items center
		position relative
		height 70px
		width 100%
	&__logo
		display block
		margin-left 30px
		height auto
		img
			width 100px
			height auto
	&__nav
		display flex
		flex-direction column
		justify-content flex-start
		align-items flex-start
		position fixed
		background-color $bg
		border-bottom 1px solid rgba($grey, .7)
		top -250px
		left 0
		width 100%
		z-index 100
		height 250px
		padding 20px 15px 15px
		transition .5s
		&--active
			top 0
	&__nav-link
		font-size 12px
		color $text1
		letter-spacing .4px
		text-transform uppercase
		line-height 22px
		height 22px
		display inline-flex
		flex-direction row
		justify-content flex-start
		align-items center
		svg
			stroke $text2
			width 22px
			height auto
			transition .5s
		&:hover
		&[aria-expanded="true"]
			color $purple
			svg
				stroke $purple
	&__nav-item
		margin-bottom 20px
		position relative
		&:last-child
			margin-bottom 0
	&__nav-menu
		display block
		position absolute
		z-index -1
		pointer-events none
		opacity 0
		top 0
		background-color $bg
		border-radius 0 0 6px 6px
		padding 20px
		width 200px
		transition .5s
		transform translate3d(0px, 21px, 0px)
		margin-top 10px
		overflow hidden
		height auto
		max-height 240px
		box-shadow 0 1px 15px 0 rgba(0, 0, 0, .12)
		li
			margin-bottom 15px
			&:last-child
				margin-bottom 0
		a
			display flex
			flex-direction row
			justify-content flex-start
			align-items center
			color $text2
			font-size 14px
			font-weight 400
			letter-spacing .4px
			transition .5s
			padding-left 20px
			position relative
			&:before
				content ''
				position absolute
				display block
				top 0
				bottom 0
				left 0
				width 10px
				background url('../img/arrow.svg') no-repeat left center
				background-size 10px auto
				opacity .5
				transition .5s
			&:hover
				color $text1
				&:before
					opacity 1
		&.show
			z-index 1000
			pointer-events auto
			opacity 1
	&__form
		display flex
		flex-direction row
		justify-content space-between
		align-items center
		height 44px
		width 220px
		background-color transparent
		border 1px solid rgba($grey, .7)
		border-radius 6px
		position relative
	&__select
		display none
	&__input
		width calc(100% - 50px)
		height 44px
		border-radius 6px 0 0 6px
		border none
		background-color transparent
		font-size 14px
		color $text1
		padding 0 20px
	&__btn
		display flex
		flex-direction row
		justify-content center
		align-items center
		width 50px
		height 44px
		position relative
		&:before
			content ''
			position absolute
			display block
			top 10px
			bottom 10px
			width 1px
			background-color rgba($grey, .7)
			left 0
			pointer-events none
		svg
			width 18px
			height auto
			fill $purple
			stroke $purple
	&__actions
		display flex
		flex-direction row
		justify-content space-between
		align-items center
		height 70px
		width 120px
		&--2
			width 60px
	&__login
		display flex
		flex-direction row
		justify-content center
		align-items center
		width 44px
		height 44px
		background-color $purple
		border-radius 6px
		svg
			stroke $white
			width 22px
			height auto
			transition .5s
		span
			display none
		&:hover
			background-color $orange
			svg
				stroke $white
	&__lang
		display flex
		flex-direction row
		justify-content flex-start
		align-items center
		position relative
		height 70px
		z-index 2
	&__lang-btn
		display flex
		flex-direction row
		justify-content flex-start
		align-items center
		img
			width 20px
			height 20px
			border-radius 50%
			margin-right 10px
			border 1px solid $purple
		span
			color $text1
			font-size 12px
			font-weight 400
			text-transform uppercase
			letter-spacing .6px
		svg
			width 10px
			height auto
			fill $purple
			margin-left 5px
	&__lang-menu
		display block
		position absolute
		z-index -1
		pointer-events none
		opacity 0
		top 0
		margin-top 26px
		background-color $bg
		border-radius 0 0 6px 6px
		border-top none
		padding 20px
		width 100px
		transition .5s
		transform translate3d(0px, 45px, 0px)
		box-shadow 0 1px 15px 0 rgba(0, 0, 0, .12)
		li
			margin-bottom 15px
			&:last-child
				margin-bottom 0
		a
			display flex
			flex-direction row
			justify-content flex-start
			align-items center
			img
				width 20px
				height 20px
				border-radius 50%
				margin-right 10px
				border 1px solid transparent
				transition .5s
			span
				color $text2
				font-size 12px
				font-weight 400
				text-transform uppercase
				letter-spacing .6px
				transition .5s
			&:hover
				span
					color $text1
				img
					border-color $purple
		&.show
			z-index 1000
			pointer-events auto
			opacity 1
	&__link
		display flex
		flex-direction row
		justify-content flex-start
		align-items center
		svg
			stroke $purple
			width 22px
			height auto
			transition .5s
		span
			display none
	&__menu
		position absolute
		width 24px
		height 22px
		display block
		left 0
		top 24px
		span
			position absolute
			left 0
			width 24px
			height 2px
			background-color $text1
			border-radius 3px
			transition .5s
			opacity 1
			&:first-child
				top 0
			&:nth-child(2)
				top 10px
				width 16px
			&:last-child
				top 20px
				width 8px
		&--active
			span
				&:first-child
					transform rotate(45deg)
					top 10px
				&:nth-child(2)
					opacity 0
				&:last-child
					width 24px
					transform rotate(-45deg)
					top 10px
	@media (min-width: 360px)
		&__logo
			margin-left 40px
		&__actions
			&--2
				width 75px
	@media (min-width: 576px)
		&__actions
			width 230px
		&__login
			width 114px
			svg
				display none
			span
				display block
				font-size 12px
				color $white
				letter-spacing .4px
				text-transform uppercase
				transition .5s
			&:hover
				span
					color $white
		&__link
			span
				display block
				font-size 12px
				color $text1
				text-transform uppercase
				letter-spacing .4px
				margin-left 10px
				transition .5s
			&:hover
				svg
					stroke $orange
		&__nav
			padding-left calc((100% - 510px)/2)
			padding-top 20px
	@media (min-width: 768px)
		&__actions
			width 240px
		&__login
			width 120px
		&__form
			width 260px
		&__nav
			padding-left calc((100% - 690px)/2)
	@media (min-width: 992px)
		&__nav
			padding-left calc((100% - 930px)/2)
	@media (min-width: 1200px)
		&__nav
			flex-direction row
			align-items center
			top auto
			left auto
			position relative
			height 70px
			padding 0
			width auto
			z-index 2
			border-bottom none
			background-color transparent
			margin-right auto
			margin-left 30px
			&:before
				content ''
				position absolute
				display block
				top 24px
				bottom 24px
				width 1px
				background-color rgba($grey, .7)
				left -29px
				pointer-events none
		&__nav-item
			display flex
			flex-direction row
			justify-content flex-start
			align-items center
			height 70px
			margin-bottom 0
			margin-right 60px
			&:last-child
				margin-right 0
		&__menu
			display none
		&__logo
			margin-left 0
			width 196px
		&__actions
			width 230px
		&__login
			width 114px
		&__form
			width 426px
			&:before
				content ''
				position absolute
				display block
				top 10px
				bottom 10px
				width 1px
				background-color rgba($grey, .7)
				left 196px
				pointer-events none
		&__select
			display block
			width 115px
			height 44px
			border none
			background-color transparent
			font-size 14px
			color $text2
			padding 0
			background url('../img/arrow2.svg') no-repeat center right
			background-size 12px auto
			cursor pointer
			option
				padding 0
				margin 0
		&__input
			width 196px
		&__nav-menu
			transform translate3d(0px, 46px, 0px)
			margin-top 25px
			border-top none
	@media (min-width: 1310px)
		&__logo
			width 212px
		&__actions
			width 262px
		&__login
			width 130px
		&__form
			width 474px
			&:before
				left 212px
		&__input
			width 212px
		&__select
			width 150px

/*==============================
	Section
==============================*/
.section
	position relative
	padding-top 60px
	&--last
		padding-bottom 60px
	&--first
		padding-top 200px
	&--bg
		background-color $bg
		overflow hidden
		.container
			position relative
			z-index 2
		&:before
			content ''
			position absolute
			display block
			z-index 1
			top 140px
			left 0
			right 0
			height 500px
			pointer-events none
			background -webkit-linear-gradient(top, rgba($bg, .1) 0%, rgba($bg, 1) 100%)
			background -ms-linear-gradient(top, rgba($bg, .1) 0%, rgba($bg, 1) 100%)
			background linear-gradient(to bottom, rgba($bg, .1) 0%, rgba($bg, 1) 100%)
	&--head
		box-shadow inset 0 -1px 15px 0 rgba(0, 0, 0, .12)
		overflow hidden
		.container
			position relative
			z-index 2
		&:before
			content ''
			position absolute
			display block
			z-index 1
			top 140px
			left 0
			right 0
			bottom 0
			pointer-events none
			background-color rgba($bg, .5)
	&--full-bg
		overflow hidden
		.container
			position relative
			z-index 2
		&:before
			content ''
			position absolute
			display block
			z-index 1
			top 0
			left 0
			right 0
			bottom 0
			pointer-events none
			background-color rgba($bg, .5)
	&--carousel
		margin-bottom -15px
	&__title-wrap
		position relative
		display flex
		flex-direction column
		justify-content flex-start
		align-items flex-start
	&__title
		color $text1
		font-weight 400
		font-size 28px
		line-height 100%
		margin-bottom 0
		position relative
		padding-left 30px
		font-family 'Montserrat', sans-serif
		b
			font-weight 500
		span
			font-size 14px
			color $text2
			font-family 'Open Sans', sans-serif
			letter-spacing .4px
		&:before
			content ''
			position absolute
			display block
			top 2px
			bottom 2px
			left 0
			width 3px
			background-color $purple
			border-radius 4px
		&--pre
			&:before
				background-color $orange
		&--title
			text-transform uppercase
			font-weight 300
			line-height 130%
		&--small
			padding-left 0
			font-size 26px
			&:before
				display none
	&__nav-wrap
		display flex
		flex-direction row
		justify-content flex-start
		align-items center
		width 100%
		margin-top 20px
	&__view
		display flex
		flex-direction row
		justify-content center
		align-items center
		width 80px
		height 30px
		color $text2
		font-size 13px
		font-weight 400
		border-radius 6px
		background-color $white
		margin-right auto
		box-shadow 0 1px 10px 0 rgba(0, 0, 0, .12)
		&:hover
			color $orange
			box-shadow 0 1px 10px 0 rgba(0, 0, 0, .2)
	&__nav
		display flex
		flex-direction row
		justify-content center
		align-items center
		width 30px
		height 30px
		background-color $white
		box-shadow 0 1px 10px 0 rgba(0, 0, 0, .12)
		border-radius 6px
		svg
			stroke $text2
			transition .5s
			width 18px
			height auto
		&:hover
			box-shadow 0 1px 10px 0 rgba(0, 0, 0, .2)
			svg
				stroke $purple
		&--bg
			background-color $bg
			&:hover
				background-color $bg
		&--prev
			margin-right 15px
	&__carousel
		position relative
		width 100%
		.card
			width 220px
			margin-bottom 15px
			margin-left 15px
			box-shadow none
			&--big
				width 270px
		.owl-item.active
			.card
				box-shadow 0 1px 15px 0 rgba(0, 0, 0, .12)
				&:hover
					box-shadow 0 1px 15px 0 rgba(0, 0, 0, .2)
	&__text
		color $text2
		font-size 16px
		line-height 26px
		margin-bottom 20px
		letter-spacing .4px
		&:last-child
			margin-bottom 0
		b
			font-weight 600
			color $text1
		a
			color $purple
			&:hover
				color $purple
				text-decoration underline
		&--mt
			margin-top 30px
	&__list
		ol
			padding-left 0
			list-style none
			counter-reset li
			margin-bottom 0
			ol
				padding-left 15px
				margin-top 10px
				ol
					margin-top 10px
					margin-bottom 10px
					li
						margin-top 5px
			h4
				font-size 16px
				font-weight 600
				color $text1
				display inline-block
				letter-spacing .4px
				margin-bottom 0
				margin-top 20px
			li
				font-size 16px
				line-height 26px
				color $text2
				position relative
				letter-spacing .4px
				b
					font-weight 600
					color $text1
				a
					color $purple
					&:hover
						color $purple
						text-decoration underline
				&:last-child
					margin-bottom 0
				&:before
					counter-increment li
					content counters(li,".") ". "
	&__wrap
		display flex
		flex-direction column
		justify-content flex-start
		align-items flex-start
	@media (min-width: 576px)
		padding-top 70px
		&--last
			padding-bottom 70px
		&--first
			padding-top 210px
		&__title
			font-size 30px
			&--small
				font-size 28px
			&--title
				line-height 100%
		&__carousel
			padding-left calc((100% - 570px)/2)
			.card
				margin-left 30px
				&--big
					width 280px
	@media (min-width: 768px)
		padding-top 80px
		&--last
			padding-bottom 80px
		&--first
			padding-top 220px
		&__title-wrap
			flex-direction row
			justify-content space-between
			align-items center
			margin-bottom 5px
		&__nav-wrap
			margin-top 0
			width auto
		&__nav
			&--prev
				margin-left 30px
		&__view
			margin-right 0
		&__title
			font-size 32px
			&:before
				top 3px
				bottom 3px
			&--small
				font-size 28px
		&__carousel
			padding-left 0
			width 720px
			margin 0 auto
			.card
				width 100%
				margin-left 0
				&--big
					width 100%
		&__wrap
			flex-direction row
			justify-content space-between
			align-items center
	@media (min-width: 992px)
		&--catalog
			padding-top 50px
		&__carousel
			width 960px
	@media (min-width: 1200px)
		padding-top 90px
		&--last
			padding-bottom 90px
		&--first
			padding-top 230px
		&--catalog
			padding-top 60px
		&__carousel
			width 1140px
	@media (min-width: 1310px)
		&__carousel
			width 1310px

/*==============================
	Breadcrumb
==============================*/
.breadcrumb
	display flex
	flex-direction row
	justify-content flex-start
	align-items center
	margin-top 30px
	&__item
		font-size 14px
		line-height 22px
		color $text2
		transition .5s
		position relative
		margin-right 40px
		letter-spacing .4px
		&:first-child
			padding-left 25px
			&:after
				content ''
				position absolute
				left 0
				top 0
				height 22px
				width 25px
				background url('../img/home.svg') no-repeat center left / 16px auto
		&:before
			content ''
			position absolute
			left 100%
			top 0
			height 22px
			width 40px
			background url('../img/breadcrumb.svg') no-repeat center / 14px auto
			opacity .8
		&:hover
			color $text2
		&--active
			cursor default
			margin-right 0
			&:before
				display none
			&:hover
				color $text2
	a
		color $text1
		&:hover
			color $orange
	@media (min-width: 768px)
		margin-top 0

/*==============================
	Paginator
==============================*/
.paginator
	display flex
	flex-direction row
	justify-content center
	align-items center
	margin-top 40px
	&__counter
		display none
	&__wrap
		display flex
		flex-direction row
		justify-content center
		align-items center
		height 44px
		max-width 100%
	&__item
		margin-right 15px
		a
			font-size 14px
			height 44px
			width 44px
			display flex
			flex-direction row
			justify-content center
			align-items center
			background-color $white
			border-radius 6px
			color $text2
			text-align center
			position relative
			box-shadow 0 1px 10px 0 rgba(0, 0, 0, .12)
		&:last-child
			margin-right 0
		&:hover
			a
				color $orange
				box-shadow 0 1px 10px 0 rgba(0, 0, 0, .2)
		&--prev
		&--next
			svg
				width 18px
				height auto
				stroke $text2
				transition .5s
			&:hover
				svg
					stroke $orange
		&--active
			cursor default
			a
				color $text1
