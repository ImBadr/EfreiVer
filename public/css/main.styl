		margin-bottom 10px
		height 30px
		padding 0 12px
		border-radius 6px
		letter-spacing .4px
		background-color rgba($grey, .12)
		div
			position relative
			&:first-child
				margin-right 16px
				&:after
					content '–'
					position absolute
					display block
					left 100%
					top 0
					color $text1
					font-size 14px
					margin-left 4px
					line-height 16px
	&__btn
